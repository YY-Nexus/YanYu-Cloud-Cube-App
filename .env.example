# YanYu Cloud Cube App - Environment Variables
# Copy this file to .env.local and fill in your values

# =================================
# Common Variables
# =================================

# Next.js Configuration
NEXT_PUBLIC_APP_NAME="YanYu Cloud Cube App"
NEXT_PUBLIC_APP_VERSION="1.0.0"
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Environment
NODE_ENV=development

# =================================
# EWM App Variables
# =================================

# Replicate API (for AI/ML services)
# Get this from: https://replicate.com/account/api-tokens
REPLICATE_API_KEY=your_replicate_api_key_here

# Vercel Blob Storage
# Get these from: https://vercel.com/docs/storage/vercel-blob/quickstart
BLOB_READ_WRITE_TOKEN=your_blob_token_here

# Vercel KV Storage
# Get these from: https://vercel.com/docs/storage/vercel-kv/quickstart
KV_URL=your_kv_url_here
KV_REST_API_URL=your_kv_rest_api_url_here
KV_REST_API_TOKEN=your_kv_rest_api_token_here
KV_REST_API_READ_ONLY_TOKEN=your_kv_rest_api_read_only_token_here

# =================================
# Supabase App Variables
# =================================

# Supabase Configuration
# Get these from: https://supabase.com/dashboard/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6ImFub24iLCJleHAiOjE5ODM4MTI5OTZ9.CRXP1A7WOeoJeXxjNni43kdQwgnWNReilDMblYTn_I0
NEXT_SITE_URL=http://localhost:3000
NEXT_REDIRECT_URLS=http://localhost:3000/

# =================================
# Analytics & Monitoring
# =================================

# Vercel Analytics
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your_analytics_id_here

# Plausible Analytics (if used)
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=your_domain_here

# =================================
# Development & Testing
# =================================

# Enable debug mode
DEBUG=false

# Enable verbose logging
VERBOSE_LOGGING=false

# Test environment database (if different from main)
TEST_DATABASE_URL=your_test_db_url_here